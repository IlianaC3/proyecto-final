<%- include('../layouts/header') -%>

<h3>Mis mensajes</h3>

<div class="row">
    <div class="col-sm-6"><a href="/chat">Regresar</a></div>
</div>
<!-- Lista de mensajes -->
<div class="row container">
    <h5>Mensajes</h5>
    <ul id="" class="scroll-y">
        <% data.forEach(function(unique) { %>
            <li class="li-style"><span class="email"><%= unique.autor.id %></span> (<span class="time"><%= unique.timestamp %></span>): <span class="message"><%= unique.text %></span> <img src="<%= unique.autor.avatar %>" style="width: 50px; height: 50px; border-radius: 50%;"> %>
        <% }) %>
    </ul>
</div>

<%- include('../layouts/footer') -%>